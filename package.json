{
  "name": "casperholders-mobile",
  "version": "1.0.4",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "test": "jest",
    "ios": "expo run:ios -d",
    "eject": "expo eject",
    "publish": "dotenv -e .env.local -- cross-var expo publish",
    "apk": "eas build --platform android --local --profile preview --output=dist/casperholders.apk",
    "aab": "eas build --platform android --local --output=dist/casperholders.aab",
    "app": "eas build --platform ios --local --profile preview --output=dist/casperholders.tar.gz",
    "ipa": "eas build --platform ios --local --output=dist/casperholders.ipa"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/ledger/**",
      "!**/torus/**",
      "!**/src/0bootstrap.js",
      "!**/src/theme.js",
      "!**/src/**/ledgerAdapter.js",
      "!**/src/**/abstract*.js",
      "!**/src/**/deployConnection.js",
      "!**/src/**/usePasteHandler.js",
      "!**/src/**/BottomSheetModalContainer.js",
      "!**/src/**/ensureNotificationsPermission.js",
      "!**/src/**/truncateInMiddle.js",
      "!**/src/**/useAsyncHandler.js",
      "!**/src/**/deployManager.js",
      "!**/src/**/useValidatorList.js",
      "!**/src/**/useEventSource.js",
      "!**/src/**/OperationsResult.js"
    ]
  },
  "dependencies": {
    "@casperholders/core": "1.14.2",
    "@config-plugins/react-native-ble-plx": "^0.0.2",
    "@expo/config-plugins": "^5.0.0",
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "^4",
    "@ledgerhq/react-native-hid": "^6.23.0",
    "@ledgerhq/react-native-hw-transport-ble": "^6.20.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/material-bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@reduxjs/toolkit": "^1.7.1",
    "@zondax/ledger-casper": "^0.0.1",
    "big.js": "^6.1.1",
    "casper-js-sdk": "^2.7.2",
    "core-js": "^3.6.5",
    "expo": "~46.0.16",
    "expo-clipboard": "~3.1.0",
    "expo-dev-client": "~1.3.1",
    "expo-location": "~14.3.0",
    "expo-notifications": "~0.16.1",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "expo-web-browser": "~11.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^46.0.0",
    "jsc-android": "^250230.2.1",
    "lodash": "^4.17.21",
    "node-libs-react-native": "^1.2.1",
    "react": "18.0.0",
    "react-content-loader": "^6.1.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-android-location-enabler": "^1.2.2",
    "react-native-ble-plx": "^2.0.3",
    "react-native-dotenv": "^3.3.1",
    "react-native-event-source": "^1.1.0",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-paper": "^4.11.1",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-reanimated": "~2.10.0",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-svg": "12.3.0",
    "react-native-web": "~0.18.7",
    "react-redux": "^7.2.6",
    "toformat": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.1.1",
    "react-test-renderer": "18.2.0"
  },
  "private": true
}
